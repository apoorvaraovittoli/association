<p id="notice"><%= notice %></p>

<h1>Birthdays</h1>

<table>
  <thead>
    <tr>
      <th>Date of birth</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @birthdays.each do |birthday| %>
      <tr>
        <td><%= birthday.date_of_birth %></td>
        <td><%= link_to 'Show', teacher_student_birthday_path(@teacher,@student,@birthday) %></td>
        <td><%= link_to 'Edit',  edit_teacher_student_birthday_path(@teacher,@student,@birthday) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

